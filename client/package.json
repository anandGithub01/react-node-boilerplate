{
  "name": "react_boiler_plate",
  "version": "1.0.0",
  "description": "A scalable, maintainable, PWA react-redux boilerplate with socket, multi-language, code splitting and many other good stuff.",
  "main": "index.js",
  "homepage": "/",
  "scripts": {
    "start:dev": "webpack-dev-server --env.mode=development --open --hot",
    "build:prod": "webpack --env.mode=production --mode production",
    "test": "jest --passWithNoTests",
    "setup": "node ./extra/scripts/setup.js"
  },
  "keywords": [
    "react",
    "hooks",
    "redux"
  ],
  "author": "Kirti Anand Singh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.7.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "dotenv": "^8.6.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^5.3.1",
    "img-loader": "^3.0.2",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "particles-bg": "^2.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-form-validator-core": "^0.6.5",
    "react-lottie": "^1.2.3",
    "react-phone-number-input": "^3.1.21",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1",
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^CommonComponents(.*)$": "<rootDir>/src/components/common$1",
      "^Constants(.*)$": "<rootDir>/src/constants$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1",
      "^Hooks(.*)$": "<rootDir>/src/hooks$1",
      "^Modules(.*)$": "<rootDir>/src/modules$1",
      "^Store(.*)$": "<rootDir>/src/store$1",
      "^Validations(.*)$": "<rootDir>/src/validations$1",
      "^Css(.*)$": "<rootDir>/src/assets/css$1"
    }
  }
}
